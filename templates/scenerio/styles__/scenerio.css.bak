/* Progress Bar*/


/*.progress-bar1 {
  position: relative;
  height: 100px;
  width: 100px;
  margin:0 auto;
}*/
 
.scenerio-bg  .progress-bar1 div {
  position: absolute;
  height: 100px;
  width: 100px;
  border-radius: 50%;
}
 
.scenerio-bg  .progress-bar1 div span {
position: absolute;
font-family: Arial;
font-size: 25px;
line-height: 90px;
height: 90px;
width: 90px;
left: 4.5px;
top: 5.5px;
text-align: center;
border-radius: 50%;
background-color: white;
}

.scenerio-bg  .progress-bar1 .background { background-color: #b3cef6; }
 
.scenerio-bg  .progress-bar1 .rotate {
  clip: rect(0 50px 100px 0);
  background-color: #4b86db;
}
 
.scenerio-bg  .progress-bar1 .left {
  clip: rect(0 50px 100px 0);
  opacity: 1;
  background-color: #b3cef6;
}
 
.scenerio-bg  .progress-bar1 .right {
  clip: rect(0 50px 100px 0);
  transform: rotate(180deg);
  opacity: 0;
  background-color: #4b86db;
}
 
@keyframes
toggle {  0% {
 opacity: 0;
}
 100% {
 opacity: 1;
}
}  

/* Progress Bar end */


